<h3>業務一覧</h3>
  <%= link_to new_task_path, class: 'text_dark btn-default' do %>
      <span>新規業務登録</span>
    <% end %>
  <% @tasks.each do |task| %>
    <%= task.execution_time %>
    <%= task.name %>
    <% if task.is_active == "有効" %>
      <span class="text-success"><strong><%= task.is_active %></strong></span>
    <% else %>
      <span class="text-secondary"><strong><%= task.is_active %></strong></span>
    <% end %>
    <%= link_to edit_task_path(task.id), class: 'btn-success' do %>
      <span>編集</span>
    <% end %>
  <% end %>